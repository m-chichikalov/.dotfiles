// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "ctrl+alt+p",
        "command": "workbench.action.openWorkspace"
    },
    {
        "key": "alt+enter",
        "command": "-python.datascience.runcurrentcellandaddbelow",
        "when": "editorTextFocus && python.datascience.featureenabled && python.datascience.hascodecells && !editorHasSelection"
    },
    {
        "key": "alt+enter",
        "command": "-editor.action.selectAllMatches",
        "when": "editorFocus && findWidgetVisible"
    },
    {
        "key": "alt+enter",
        "command": "-debug.openBreakpointToSide",
        "when": "breakpointsFocused"
    },
    {
        "key": "alt+enter",
        "command": "workbench.action.openWorkspaceConfigFile"
    },
    {
        "key": "ctrl+insert",
        "command": "explorer.newFile"
    },
    {
        "key": "ctrl+shift+insert",
        "command": "explorer.newFolder"
    },
    {
        "key": "alt+right",
        "command": "-workbench.action.terminal.focusNextPane",
        "when": "terminalFocus"
    },
    {
        "key": "alt+right",
        "command": "-gitlens.key.alt+right",
        "when": "gitlens:key:alt+right"
    },
    {
        "key": "alt+right",
        "command": "workbench.action.navigateForward"
    },
    {
        "key": "ctrl+shift+-",
        "command": "-workbench.action.navigateForward"
    },
    {
        "key": "alt+left",
        "command": "workbench.action.navigateBack"
    },
    {
        "key": "ctrl+alt+-",
        "command": "-workbench.action.navigateBack"
    },
    {
        "key": "alt+left",
        "command": "workbench.action.quickInputBack",
        "when": "inQuickOpen"
    },
    {
        "key": "ctrl+alt+-",
        "command": "-workbench.action.quickInputBack",
        "when": "inQuickOpen"
    },
    {
        "key": "alt+left",
        "command": "-workbench.action.terminal.focusPreviousPane",
        "when": "terminalFocus"
    },
    {
        "key": "alt+left",
        "command": "-gitlens.key.alt+left",
        "when": "gitlens:key:alt+left"
    },
    {
        "key": "ctrl+alt+h",
        "command": "bookmarks.listFromAllFiles"
    },
    {
        "key": "ctrl+alt+;",
        "command": "bookmarks.list"
    },
    {
        "key": "ctrl+alt+s",
        "command": "bookmarks.selectLines"
    },
    {
        "key": "ctrl+alt+q",
        "command": "bookmarks.clear"
    },
    {
        "key": "ctrl+alt+w",
        "command": "bookmarks.clearFromAllFiles"
    },
    {
        "key": "shift+alt+l",
        "command": "-bookmarks.expandSelectionToNext",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+j ctrl+alt+l",
        "command": "workbench.view.extension.bookmarks"
    },
    {
        "key": "ctrl+k ctrl+a",
        "command": "workbench.action.focusActiveEditorGroup"
    },
    {
        "key": "ctrl+h ctrl+h",
        "command": "workbench.explorer.fileView.focus"
    },
    {
        "key": "ctrl+shift+q",
        "command": "workbench.action.toggleMaximizedPanel"
    }
]